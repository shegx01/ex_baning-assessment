{
  "version": "2.0.0",
  "command": "mix",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "args": [
        "compile"
      ],
      "problemMatcher": [
        "$mixCompileError",
        "$mixCompileWarning"
      ],
      "group": {
        "_id": "build",
        "isDefault": false
      }
    },
    {
      "label": "test",
      "type": "shell",
      "args": [
        "test"
      ],
      "problemMatcher": [
        "$mixCompileError",
        "$mixCompileWarning",
        "$mixTestFailure"
      ],
      "group": {
        "_id": "test",
        "isDefault": false
      }
    }
  ]
}
